    # The folder is used to store method traces.
    chmod 0773 /data/misc/trace
    # Give reads to anyone for the window trace folder on debug builds.
    chmod 0775 /data/misc/wmtrace

# Samsung Flow USB Connection daemon
    service ss_conn_daemon2_service /system/bin/ss_conn_daemon2
    class main
    socket ss_conn_daemon2 stream 0660 system system
    user system
    group inet net_raw
    disabled

# LPM
service lpm /system/bin/lpm
    class sec-charger
    user root
    group system radio input
    critical

# barnswallow daemon
service bsd /system/bin/bsd
    class main
    user system
    group system
    disabled

# insthk
service insthk /system/bin/insthk
    class core
    user system
    group system
    disabled
    oneshot

on property:vendor.sys.qseecomd.enable=true
    start insthk

on init && property:ro.debuggable=1
    start console

# Stork Daemon
service storkd /vendor/bin/storkd
    class core
    user system
    group system
    disabled

service scs /system/bin/scs
    class main
    user root
    group root system
    oneshot
    disabled

on userspace-reboot-requested
  # TODO(b/135984674): reset all necessary properties here.
  setprop sys.boot_completed ""
  setprop dev.bootcomplete ""
  setprop sys.init.updatable_crashing ""
  setprop sys.init.updatable_crashing_process_name ""
  setprop apexd.status ""
  setprop sys.user.0.ce_available ""
  setprop sys.shutdown.requested ""
  setprop service.bootanim.exit ""

on userspace-reboot-fs-remount
  # Make sure that vold is running.
  # This is mostly a precaution measure in case vold for some reason wasn't running when
  # userspace reboot was initiated.
  start vold
  exec - system system -- /system/bin/vdc checkpoint resetCheckpoint
  exec - system system -- /system/bin/vdc checkpoint markBootAttempt
  # Unmount /data_mirror mounts in the reverse order of corresponding mounts.
  umount /data_mirror/data_ce/null/0
  umount /data_mirror/data_ce/null
  umount /data_mirror/data_de/null
  umount /data_mirror/cur_profiles
  umount /data_mirror
  remount_userdata
  start bootanim

on userspace-reboot-resume
  trigger userspace-reboot-fs-remount
  trigger post-fs-data
  trigger zygote-start
  trigger early-boot
  trigger boot

on property:sys.boot_completed=1 && property:sys.init.userspace_reboot.in_progress=1
  setprop sys.init.userspace_reboot.in_progress ""

## SecProductFeature_KNOX.SEC_PRODUCT_FEATURE_KNOX_SUPPORT_MDM {
## EDM AuditLog
service edmaudit /system/bin/edmaudit
    class main
    user system
    group system
    capabilities SYSLOG
    disabled

on property:security.edmaudit=true
    start edmaudit

on property:security.edmaudit=false
    stop edmaudit
## SecProductFeature_KNOX.SEC_PRODUCT_FEATURE_KNOX_SUPPORT_MDM }

on property:sys.dexopt.ctrl=true
    write /dev/cpuset/dexopt/cpus "0-3"

on property:sys.dexopt.ctrl=false
    write /dev/cpuset/dexopt/cpus "0-7"

# IO Daemon
# IOPP-iod-android-v1.0
service iod /vendor/bin/iod
    user system
    group shell
    class main
    oneshot
    disabled

# strongSwan VPN
service charon /system/bin/charon
    class main
    socket charon stream 600 system system
    # charon will setuid to vpn after getting necessary resources.
    user vpn
    group vpn inet shell
    capabilities NET_ADMIN NET_BIND_SERVICE NET_RAW
    disabled
    oneshot

# Myfilter preload filters. If persistent property myfilter_preloaded is not set yet, then copy filter data and mark the property as done.
on property:persist.sys.myfilter_preload=
    copy  /system/cameradata/myfilter/myfilter_sample_01.sel /data/DownFilters/MyFilter/myfilter_sample_01.sel
    copy  /system/cameradata/myfilter/myfilter_sample_02.sel /data/DownFilters/MyFilter/myfilter_sample_02.sel
    copy  /system/cameradata/myfilter/myfilter_sample_03.sel /data/DownFilters/MyFilter/myfilter_sample_03.sel
    copy  /system/cameradata/myfilter/myfilter_sample_04.sel /data/DownFilters/MyFilter/myfilter_sample_04.sel
    copy  /system/cameradata/myfilter/myfilter_sample_01.bmp /data/DownFilters/MyFilter/myfilter_sample_01.bmp
    copy  /system/cameradata/myfilter/myfilter_sample_02.bmp /data/DownFilters/MyFilter/myfilter_sample_02.bmp
    copy  /system/cameradata/myfilter/myfilter_sample_03.bmp /data/DownFilters/MyFilter/myfilter_sample_03.bmp
    copy  /system/cameradata/myfilter/myfilter_sample_04.bmp /data/DownFilters/MyFilter/myfilter_sample_04.bmp
    chown system system /data/DownFilters/MyFilter/myfilter_sample_01.sel
    chown system system /data/DownFilters/MyFilter/myfilter_sample_02.sel
    chown system system /data/DownFilters/MyFilter/myfilter_sample_03.sel
    chown system system /data/DownFilters/MyFilter/myfilter_sample_04.sel
    chown system system /data/DownFilters/MyFilter/myfilter_sample_01.bmp
    chown system system /data/DownFilters/MyFilter/myfilter_sample_02.bmp
    chown system system /data/DownFilters/MyFilter/myfilter_sample_03.bmp
    chown system system /data/DownFilters/MyFilter/myfilter_sample_04.bmp
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_01.sel
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_02.sel
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_03.sel
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_04.sel
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_01.bmp
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_02.bmp
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_03.bmp
    chmod 0664 /data/DownFilters/MyFilter/myfilter_sample_04.bmp
    setprop persist.sys.myfilter_preload preload_done

# tracking activation VNG
on property:ro.csc.sales_code=XXV
    tracking_activation_VNG	

#//[@VOLD Prevent watchdog reset at vold shutdown
on property:sys.vold.needed=1
    # VOLD was not existed when VDC requested VOLD Binder.
    # so, start it again
    start vold
#//@VOLD Prevent watchdog reset at vold shutdown]

# Pageboostd
on property:sys.boot_completed=1
    mkdir /data/misc/pageboost 0771 system system

# hide carrier apps
on post-fs-data && property:ro.csc.sales_code=*
    mount none /system/carrier/${ro.csc.sales_code} /system/carrier bind
